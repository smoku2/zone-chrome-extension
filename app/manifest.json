{
    "name": "ZONE",
    "version": "0.0.17",
    "manifest_version": 2,
    "description": "エンジニアの集中ステータスをチームで共有",
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "background": {
        "page": "capturing.html"
    },
    "browser_action": {
        "default_icon": {
            "19": "images/icon-19.png",
            "38": "images/icon-38.png"
        },
        "default_title": "ZONE",
        "default_popup": "popup.html"
    },
    "permissions": [
        "http://dmtc-baio.herokuapp.com/*"
    ],
    "options_page": "capturing.html",
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}